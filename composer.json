{
  "name": "bonecms/laravel-query-signer",
  "description": "Query signer for the Laravel",
  "keywords": ["signer", "query signer", "request signer"],
  "license": "MIT",
  "homepage": "https://github.com/igoshev/laravel-query-signer",
  "authors": [
    {
      "name": "Dmitriy Igoshev",
      "email": "dmitry.igo@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "illuminate/support": "^5.6|^6.0"
  },
  "autoload": {
    "psr-4": {
      "Bone\\Signer\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1-dev"
    },
    "laravel": {
